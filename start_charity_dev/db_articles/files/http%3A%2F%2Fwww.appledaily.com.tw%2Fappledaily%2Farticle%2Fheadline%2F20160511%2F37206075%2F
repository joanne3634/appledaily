        function setimgpv(){
            $.ajaxSetup({ cache: false });
            $.ajax({
                  url: 'http://weblogging.appledaily.com.tw/api/SetClick/n/n/37206075/5/9499/20160511/appledaily/'+Math.random(),
                  dataType: 'jsonp',
