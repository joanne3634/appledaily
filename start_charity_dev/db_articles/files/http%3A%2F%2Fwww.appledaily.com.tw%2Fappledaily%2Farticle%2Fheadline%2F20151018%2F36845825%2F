        function setimgpv(){
            $.ajaxSetup({ cache: false });
            $.ajax({
                  url: 'http://weblogging.appledaily.com.tw/api/SetClick/n/n/36845825/5/9499/20151018/appledaily/'+Math.random(),
                  dataType: 'jsonp',
